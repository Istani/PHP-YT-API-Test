HOME<br>
<hr>

<div class="game_list-container">
{{#each games}}
  <a href="/game/{{name}}" class="game_link">
  <div class="game_list sk_ui-corner-all">
    <img src="{{localBanner}}" height="129" width="276"><br>
    {{display_name}}<br>
    <br><p class="price">
    {{#checkPrice lowPrice highPrice}}
      <span class="low">{{lowPrice}}&nbsp;€</span> <span class="high">{{highPrice}}&nbsp;€</span>
    {{else}}
      <span class="">{{lowPrice}}&nbsp;€</span>
    {{/checkPrice}}
    </p>
  </div>
  </a>
{{/each}}
</div>
