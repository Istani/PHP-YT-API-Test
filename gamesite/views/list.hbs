<table class="game_list-container">
  <thead>
    <tr>
      <td></td>
    </tr>
  </thead>
  <tbody>
    {{#each games}}
      <tr class="game_list corner-all">
        <td>
          <a href="/game/{{name}}" class="game_link">
            <div>
              <img data-src="{{banner}}" src="/img/games/no_pic.jpg" height="129" width="276" class="lazyload" /><br>
              <span class="display_name">{{display_name}}</span><br>
              <br>
              <p class="price">
                {{#checkPrice games.lowPrice games.highPrice}}
                  <span class="low">{{games.lowPrice}}&nbsp;€</span> <span class="high">{{games.highPrice}}&nbsp;€</span>
                {{else}}
                  <span class="">{{games.lowPrice}}&nbsp;€</span>
                {{/checkPrice}}
              </p>
            </div>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
